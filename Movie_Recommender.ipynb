    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "\n",
    "tf_gs =TfidfVectorizer(ngram_range=(1,1)) # unigrams\n",
    "text_tf_gs= tf_gs.fit_transform(genre_and_sentiment).toarray()\n",
    "Sim = cosine_similarity(text_tf_gs, text_tf_gs)\n",
