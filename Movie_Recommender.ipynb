   "source": [
    "# Module 1\n",
    "## Stemming, cleaning, removing stopwords, removing non-alphanumeric characters, and converting to lowercase the reviews in dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "stemmer = PorterStemmer()\n",
    "\n",
    "reviews = []\n",
    "reviews = df[['review']]\n",
    "reviews = reviews.replace(regex={r'[^A-Za-z0-9]': ' '})\n",
    "reviews = reviews['review'].str.lower()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "tok_reviews = []\n",
    "stop_wrd = stopwords.words('english')\n",
    "stop_wrd.append(\"br\")\n",
    "\n",
    "for w in range(50000):\n",
    "    tok_reviews.append(word_tokenize(reviews[w])) # Appending words, tokenizing\n",
    "\n",
    "for word in tok_reviews:\n",
    "    if word in stop_wrd:\n",
    "        tok_reviews.remove(word) # Removing stopwords\n",
    "        \n",
    "        \n",
    "stem_reviews = []\n",
    "for sentence in tok_reviews:\n",
    "    stem_reviews.append(\" \".join([stemmer.stem(i) for i in sentence])) # Stemming words (After stopwords removal)\n",
    "for i in range(50000):\n",
    "    df['review'][i] = stem_reviews[i]"
   ]
